10 REM **************
20 REM ** サンプル **
30 REM **************
100 X = 20:S = 0
110 CYCLE 80
120 *START
130 SYNC
200 A = STICK(0)
210 S = S + 10
250 LOCATE X, 1:PRINT "  ";
300 IF A = 7 AND X > 10 THEN X = X - 1
400 IF A = 3 AND X < 28 THEN X = X + 1
600 LOCATE 9, 24:PRINT "]                    [";
660 LOCATE RND * 20 + 10, 24:PRINT "*"
670 LOCATE X, 1:PRINT "<>";
680 LOCATE 0, 0:PRINT "SCORE " + STR$(S);
700 IF A <> 1 THEN GOTO START
1000 END
